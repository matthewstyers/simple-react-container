{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "postinstall": "npm run clean; npm run build;",
    "start": "nodemon server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.js'",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewstyers/react-redux-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "babel-preset-stage-1": "6.1.18",
    "chai-jquery": "2.0.0",
    "chai": "3.4.0",
    "chai": "3.5.0",
    "css-loader": "0.23.0",
    "cssnext-loader": "1.0.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.1",
    "eslint": "2.2.0",
    "file-loader": "0.8.4",
    "jquery": "2.2.1",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "nodemon": "1.8.1",
    "postcss-loader": "0.8.0",
    "react-addons-test-utils": "0.14.7",
    "react-hot-loader": "1.3.0",
    "rimraf": "2.4.3",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6",
