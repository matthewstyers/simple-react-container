{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "scripts": {
    "postinstall": "npm run clean; npm run build;",
    "build": "NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "cover": "istanbul cover _mocha -- --opts ./mocha.opts 'src/tests/*.test.js'",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "cover": "istanbul cover -x '*.test.js' _mocha -- --opts ./tests/mocha.opts tests/**/*.js src/**/*.js && istanbul check-coverage --lines=70",
    "test": "npm run lint && mocha --opts ./tests/mocha.opts && npm run cover",
    "test:watch": "mocha --opts ./tests/mocha.opts --watch"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "3.4.0",
    "istanbul": "0.4.0",
    "jsdom": "7.0.2",
    "mocha": "2.3.3",
    "nodemon": "1.8.1",
    "webpack-dev-middleware": "1.2.0",
    "webpack-dev-server": "1.12.1",
    "webpack-hot-middleware": "2.5.0"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-core": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "basscss": "8.0.1",
    "css-loader": "0.23.1",
    "eslint": "2.3.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.1",
    "express": "4.13.4",
    "file-loader": "0.8.5",
    "history": "2.0.1",
    "immutable": "3.7.6",
    "json-loader": "0.5.4",
    "postcss-loader": "0.8.1",
    "radium": "0.16.6",
    "react": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "react-dom": "0.14.7",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.0.0",
    "react-router": "2.0.0",
    "redux": "3.3.1",
    "redux-form": "4.2.2",
    "redux-localstorage": "0.4.0",
    "redux-logger": "2.6.1",
    "redux-router": "1.0.0-beta7",
    "redux-thunk": "2.0.1",
    "rimraf": "2.5.2",
    "require-hacker": "2.1.3",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "winston": "2.2.0"
  }
}
